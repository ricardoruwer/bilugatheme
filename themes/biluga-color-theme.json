  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#4A5568"
      }
    },
    {
      "scope": [
        "variable.other.property",
        "meta.object-literal.key",
        "punctuation.terminator",
        "support.constant.property-value",
        "support.function",
        "constant.language.symbol",
        "variable.other.block",
        "punctuation.definition.tag",
        "support.constant.font-name",
        "meta.function-call entity.name.function",
        "source.elixir.embedded",
        "meta.embedded",
        "source.groovy.embedded",
        "meta.template.expression",
        "parameter.variable"
      ],
      "settings": {
        "foreground": "#EDF2F7"
      }
    },
    {
      "scope": ["comment.unused"],
      "settings": {
        "fontStyle": "",
        "foreground": "#718096"
      }
    },
    {
      "scope": [
        "keyword",
        "storage",
        "storage.type",
        "storage.modifier",
        "support.constant.media",
        "entity.name.tag",
        "meta.embedded variable.other.global"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#FEB2B2"
      }
    },
    {
      "scope": [
        "keyword.control",
        "meta.tag.structure entity.name.tag",
        "meta.method.declaration meta.var storage.type",
        "variable.other.global punctuation.definition.variable"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#FC8181"
      }
    },
    {
      "scope": ["support.function.variable.quoted.single.elixir", "invalid"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#C53030"
      }
    },
    {
      "scope": [
        "variable.other.constant",
        "entity.other.attribute-name",
        "meta.function-call variable.other.object",
        "support.function.builtin",
        "meta.embedded variable.other"
      ],
      "settings": {
        "foreground": "#FAF089"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.other.attribute-name.class.css",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#F6E05E"
      }
    },
    {
      "scope": [
        "variable.other.readwrite",
        "keyword.operator",
        "meta.method.declaration storage.type",
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#FBD38D"
      }
    },
    {
      "scope": [
        "punctuation.definition.variable",
        "punctuation.definition.markdown",
        "punctuation.definition.raw.markdown",
        "constant.language",
        "constant.numeric",
        "meta.preprocessor.numeric"
      ],
      "settings": {
        "foreground": "#F6AD55"
      }
    },
    {
      "scope": [
        "keyword.other.unit",
        "punctuation.section.embedded",
        "punctuation.section.regexp"
      ],
      "settings": {
        "foreground": "#ED8936"
      }
    },
    {
      "scope": ["constant.other.symbol"],
      "settings": {
        "foreground": "#C6F6D5"
      }
    },
    {
      "scope": [
        "string",
        "string.tag",
        "string.value",
        "string.regexp",
        "meta.preprocessor.string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#9AE6B4"
      }
    },
    {
      "scope": ["punctuation.section.regexp", "support.class"],
      "settings": {
        "foreground": "#68D391"
      }
    },
    {
      "scope": [
        "variable.parameter",
        "variable.language",
        "variable.other.object",
        "variable.other.readwrite.js",
        "variable.scss",
        "meta.arrow meta.function-call variable.other.object"
      ],
      "settings": {
        "foreground": "#BEE3F8"
      }
    },
    {
      "scope": [
        "punctuation.separator",
        "punctuation.accessor",
        "punctuation.section",
        "support.type.property-name",
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.constant",
        "markup.changed"
      ],
      "settings": {
        "foreground": "#90CDF4"
      }
    },
    {
      "scope": [
        "meta.brace",
        "header",
        "markup.heading",
        "meta.preprocessor",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#63B3ED"
      }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#F687B3"
      }
    },
    {
      "scope": ["entity.name.function", "entity.name.section.group-title"],
      "settings": {
        "foreground": "#D6BCFA"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    },
    {
      "scope": ["support.class"],
      "settings": {
        "foreground": "#FAF089"
      }
    },
    {
      "scope": ["meta.function-call entity.name.function", "support.function"],
      "settings": {
        "foreground": "#E9D8FD"
      }
    },
    {
      "scope": ["meta.definition.function entity.name.function"],
      "settings": {
        "foreground": "#B794F4"
      }
    },
    {
      "scope": ["constant.language.symbol.hashkey"],
      "settings": {
        "foreground": "#BEE3F8"
      }
    },
    {
      "scope": [
        "variable.language",
        "support.function.core",
        "variable.parameter.function"
      ],
      "settings": {
        "foreground": "#EDF2F7"
      }
    },
    {
      "scope": ["meta.embedded meta.function-call support.function.core"],
      "settings": {
        "foreground": "#FBB6CE"
      }
    },
    {
      "scope": ["meta.function-call support.function.core"],
      "settings": {
        "foreground": "#E9D8FD"
      }
    },
    {
      "scope": ["constant.language.symbol"],
      "settings": {
        "foreground": "#C6F6D5"
      }
    }
  ]
}
